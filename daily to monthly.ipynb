    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('daily H1N1.csv')\n",
    "\n",
    "df['date'] = pd.to_datetime(df['date'])\n",
    "\n",
    "monthly_df = df.groupby(pd.Grouper(key='date', freq='M'))['new cases'].sum().reset_index()\n",
    "\n",
    "monthly_df.to_csv('monthly H1N1.csv', index=False)"
   ]
